
   <li class="listReset"><h2 class="title">{{name}}</h2></li> 
<li class="flagStyles listReset"><img src="{{flag}}" alt="flag" class="img"></li>
        <li class="infoItem">
            <h3>Capital:</h3>
            <p class="infoText">{{capital}}:</p>
        </li>
        <li class='infoItem'>
            <h3>Population:</h3>
            <p class="infoText">{{population}}:</p>
        </li>
        <li class='listReset'>
            <h3>Languages:</h3>
            <ul>
                {{#each languages}}
                <li>{{this.name}}</li>
                {{/each}}
            </ul>
        </li>

    


